nums1 = [1,2]
nums2 = [3,4]
res=[]
i=0
j=0
while i<len(nums1) and j<len(nums2):
    if nums1[i]<nums2[j]:
        res.append(nums1[i])
        i+=1
    else:
        res.append(nums2[j])
        j+=1
while i<len(nums1):
    res.append(nums1[i])
    i+=1
while j<len(nums2):
    res.append(nums2[j])
    j+=1
print("merged array is:",res)
n=len(res)
mid=n//2
if n%2==0:
    print("median is:",(res[mid-1]+res[mid])/2)
else:
    print("median is:",res[mid])
        
    
