arr=list(map(int,input("Enter only 0's, 1's, 2's:" ).split()))
low=0
mid=0
high=len(arr)-1
while mid<=high:
    if arr[mid]==0:
        arr[low],arr[mid]=arr[mid],arr[low]
        low+=1
        mid+=1
    elif arr[mid]==1:
        mid+=1
    elif arr[mid]==2:
        arr[high],arr[mid]=arr[mid],arr[high]
        high-=1
print("Array after sorting:",arr)
