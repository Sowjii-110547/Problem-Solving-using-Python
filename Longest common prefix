def longest_common_prefix(s):
    if not s:
        return ""
    prefix=s[0]
    for i in s[1:]:
        while not i.startswith(prefix):
            prefix=prefix[:-1]
        if not prefix:
            return ""
            break
    return prefix
s=['flower','flow','flight']
print("The longest common prefix is:",longest_common_prefix(s))
        
