class Node:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

class LinkedList:
    def __init__(self):
        self.head = None

    def insertnode(self, data):
        newnode = Node(data)
        if self.head is None:
            self.head = newnode
        else:
            temp = self.head
            while temp.next:
                temp = temp.next
            temp.next = newnode

    def display(self):
        t = self.head
        l = []
        while t:
            l.append(t.val)
            t = t.next
        print("Linked List:", l)

    def delete_from_end(self, n):
        first = self.head
        second = self.head
        while n > 0:
            if first is None:
                print("Index out of range")
                return
            first = first.next
            n -= 1
        if first is None:
            self.head = self.head.next
            return
        while first.next:
            first = first.next
            second = second.next
        second.next = second.next.next

# --------- Test Cases ---------
def test_cases():
    print("Test Case 1: Delete 2nd node from end")
    ob = LinkedList()
    for i in range(1, 6):  # List: [1, 2, 3, 4, 5]
        ob.insertnode(i)
    ob.display()
    ob.delete_from_end(2)  # Deletes 4
    ob.display()
    print()

    print("Test Case 2: Delete head (5th from end)")
    ob = LinkedList()
    for i in range(1, 6):  # List: [1, 2, 3, 4, 5]
        ob.insertnode(i)
    ob.display()
    ob.delete_from_end(5)  # Deletes 1
    ob.display()
    print()

    print("Test Case 3: Delete last node")
    ob = LinkedList()
    for i in range(1, 6):  # List: [1, 2, 3, 4, 5]
        ob.insertnode(i)
    ob.display()
    ob.delete_from_end(1)  # Deletes 5
    ob.display()
    print()

    print("Test Case 4: Delete with index out of range")
    ob = LinkedList()
    for i in range(1, 4):  # List: [1, 2, 3]
        ob.insertnode(i)
    ob.display()
    ob.delete_from_end(5)  # Invalid
    ob.display()
    print()

test_cases()
